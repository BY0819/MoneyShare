<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vmfld.moneyshare">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".MakeRoomActivity"
            android:label="@string/title_activity_make_room"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".RoomActivity"
            android:label="@string/title_activity_room"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ManageMoneyActivity"
            android:label="@string/title_activity_manage_money"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".HistoryActivity"
            android:label="@string/title_activity_history"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".PhotoActivity"
            android:label="@string/title_activity_photo"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".GalleryActivity"
            android:label="@string/title_activity_gallery"
            android:theme="@style/AppTheme"/>
        <activity
            android:name=".divide.DivideActivity"
            android:label="@null"
            android:theme="@style/AppTheme"/>
    </application>

</manifest>